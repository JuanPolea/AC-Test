<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:name=".ProjectApplication"
        android:allowBackup="true"
        android:exported="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ACTest">
        <activity
            android:name="android.app.Activity"
            android:theme="@style/Theme.ACTest.NoActionBar" />
        <activity android:name="androidx.activity.ComponentActivity" />
        <activity
            android:name="com.jfmr.ac.test.presentation.ui.main.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ACTest.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
